{
  "name": "cbor2",
  "version": "0.0.0",
  "description": "Encode and parse data in the Concise Binary Object Representation (CBOR) data format (RFC8949).",
  "main": "./lib/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "http://github.com/hildjj/cbor2.git"
  },
  "homepage": "https://github.com/hildjj/cbor2",
  "scripts": {
    "clean": "rimraf lib coverage docs",
    "docs": "typedoc",
    "lint": "eslint . --ext ts,js,cjs",
    "pretest": "npm run build",
    "test": "npm run test:noBuild",
    "test:noBuild": "c8 node --test",
    "build": "tsup",
    "watch": "tsup --watch src/ --watch test/ --onSuccess 'npm run test:noBuild'",
    "ci": "npm run clean && npm run build && npm run lint && npm run test:noBuild"
  },
  "keywords": [
    "coap",
    "cbor",
    "json",
    "rfc7049",
    "rfc8949"
  ],
